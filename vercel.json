{
    "builds": [
      {
        "src": "shortner_app/main.py",
        "use": "@vercel/python"
      }
    ],
    "functions": {
      "shortner_app/main.py": {
          "maxDuration": 20
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "shortner_app/main.py"
      }
    ]
  }