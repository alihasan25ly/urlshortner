{
    "devCommand": "uvicorn shortner_app.main:app --host 0.0.0.0 --port 3000",
    "builds": [
      {
        "src": "shortner_app/index.py",
        "use": "@vercel/python"
      }
    ],
    "env": {    
       "PYTHONPATH": "shortner_app/"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "shortner_app/index.py"
      }
    ]
  }